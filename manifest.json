{
  "manifest_version": 3,
  "name": "Dark Theme for Codeforces",
  "version": "1.0",
  "description": "Dark theme for the codeforces.com",
  "content_scripts": [
    {
      "matches": [
        "*://codeforces.com/*",
        "*://*.codeforces.com/*"
      ],
      "js": [
        "inject-inline.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Toggle cf-theme"
  }
}